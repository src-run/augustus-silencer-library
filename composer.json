{
  "name": "src-run/augustus-silencer-library",
  "license": "MIT",
  "type": "library",
  "homepage": "https://src.run/augustus-silencer-library/home",
  "description": "A simple library to silence php-raised errors and execute a closure in an error-silenced environment.",
  "support": {
    "email": "rmf@src.run",
    "source": "https://src.run/augustus-silencer-library/git",
    "issues": "https://src.run/augustus-silencer-library/issues"
  },
  "keywords": [ "augustus-silencer-library", "augustus", "file", "library", "rmf", "src-run" ],
  "authors": [
    {
      "name": "Rob Frawley 2nd",
      "email": "rmf@src.run",
      "homepage": "https://src.run/rmf",
      "role": "Project Lead"
    }
  ],
  "require": {
    "php": "~5.6 || ~7.0",
    "src-run/augustus-exception-library": "~0.6",
    "src-run/augustus-polyfill-library": "~0.6"
  },
  "require-dev": {
    "codacy/coverage": "~1.0",
    "friendsofphp/php-cs-fixer": "~2.0",
    "phpunit/phpunit": "~5.4",
    "satooshi/php-coveralls": "~1.0",
    "sllh/php-cs-fixer-styleci-bridge": "~2.1"
  },
  "autoload": {
    "psr-4": {
      "SR\\Silencer\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SR\\Silencer\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7-dev"
    }
  }
}
